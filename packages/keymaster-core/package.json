{
  "name": "@keekuun/keymaster-core",
  "version": "0.4.4",
  "private": false,
  "description": "keymaster 核心模块（React 和 Vue 共享的类型、解析器和工具函数）",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "umd": {
        "default": "./dist/index.umd.js"
      }
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "keywords": [
    "keymaster",
    "keyboard",
    "shortcut",
    "hotkey",
    "core",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Keekuun/keymaster.git",
    "directory": "packages/keymaster-core"
  },
  "homepage": "https://keymaster-docs.vercel.app/core/",
  "bugs": {
    "url": "https://github.com/Keekuun/keymaster/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix",
    "test": "vitest",
    "test:run": "vitest run"
  }
}
